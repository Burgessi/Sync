<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pro.sync.team.dao.TeamDaoImpl">

	<select id="getAllTeam" resultType="TeamVo">
			SELECT *
				FROM TEAM
	</select>

	<select id="getOneTeam" resultType="TeamVo">
		SELECT *
			FROM TEAM
			WHERE TEAM_CODE = #{team_code}
	</select>

</mapper>
